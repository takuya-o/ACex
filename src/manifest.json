{
  "manifest_version": 2,
  "short_name": "ACex",
  "name": "__MSG_ACex_name__",
  "description": "__MSG_ACex_description__",
  "author": "Takuya Ono",
  "version": "0.0.9.2",
  "minimum_chrome_version": "37",

  "permissions": [
    "identity",
    "tabs",
    "https://www.bbt757.com/svlAC/",
    "storage", "unlimitedStorage",
    "https://www.googleapis.com/"
  ],
  "oauth2": {
    "client_id": "942009117399-avs1lkpaf4t09cshpgrbamv4rnsm67vh.apps.googleusercontent.com",
    "scopes": [ "https://www.googleapis.com/auth/chromewebstore.readonly" ]
  },
   "key": "",
  "content_scripts": [ {
      "matches": ["https://www.bbt757.com/ac/web/*", "http://accontent.bbt757.com/content/*", "*://*.aircamp.us/course/*"],
    "js": ["lib/prototype.js", "tagmanager.js", "messageUtil.js", "ACex.js"],
    "run_at": "document_end"
  } ],
  "page_action": {
    "default_icon": "icon_19.png",
    "default_title": "AirCumpus for Web Extender",
    "default_popup": "popup.html"
  },
  "content_security_policy":
          "script-src 'self' 'unsafe-eval' https://www.googletagmanager.com https://www.google-analytics.com https://www.google.com/jsapi https://www.google.com/uds/; object-src 'self'",
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png",
    "19": "icon_19.png",
    "16": "icon_16.png"
  },
  "background": {
    "page": "background.html"
  },
  "default_locale": "ja"
}
